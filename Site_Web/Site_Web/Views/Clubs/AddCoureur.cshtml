@using Site_Web.App_Data;
@model Site_Web.App_Data.CLUB
@{
    ViewBag.Title = "AddCoureur";    
}

<h2>Ajouter un coureur</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
        <table class="tableCoureur">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.COU_PRENOM)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.COU_NOM)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.COU_EMAIL)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.COU_PRENOM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.COU_NOM)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.COU_EMAIL)
                    </td>
                  
                    <td>
                       <!-- checkbox add-->
                    </td>
                </tr>
            }

        </table>
    }



}
